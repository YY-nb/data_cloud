<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.project.data_cloud_server.mapper.FolderMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.project.data_cloud_server.entity.Folder">
        <id column="id" property="id" />
        <result column="pid" property="pid" />
        <result column="name" property="name" />
        <result column="file_store_id" property="fileStoreId" />
        <result column="create_time" property="createTime" />
        <result column="update_time" property="updateTime" />
        <result column="folder_path" property="folderPath" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, pid, name, file_store_id, create_time, update_time, folder_path
    </sql>

</mapper>
